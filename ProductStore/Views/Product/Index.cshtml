@using ProductStore.Models.Entities
@model IEnumerable<Product>

@{
    ViewData["Title"] = "Product";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Product catalogue</title>
</head>
<body>
    <ul>
        @foreach (Product p in Model)
        {
            <li>@p.Name</li>
        }
    </ul>
</body>
</html>